    "token": "secret",
    "username": "user",
    "password": "secret",
    "kv": {
      "enabled": true,
      "engineVersion": 2,
      "mountPoint": "kv",
      "path": "inventory-service/settings"
    },
    "pki": {
      "enabled": true,
      "roleName": "inventory-service",
      "commonName": "inventory-service.FoodRocket.io"
    },
    "lease": {
      "mongo": {
        "type": "database",
        "roleName": "inventory-service",
        "enabled": true,
        "autoRenewal": true,
